@model List<Product>
<div class="container">
    <div class="row pt-4">
        <div class="row col-6">
            <h2 class="text-dark">
                Person list
            </h2>
        </div>
        <div class="col-6 text-end">
            <a asp-controller="Product" asp-action="create" class="btn btn-primary">
                <i class="bi bi-plus-circle-dotted"></i> Create new product
            </a>
        </div>
    </div>
    <table class="table table-bordered table-striped">
    <thead>
        <tr>
            <th>
                Display order
            </th>
            <th>
                Category Name
            </th>
            <th>
                Preço
            </th>           
        </tr>
    </thead>
    <tbody>
        @foreach(var obj in Model.OrderBy(u=> u.Id))
        {
         <tr>
             <td>@obj.Id</td>
            <td>@obj.Name</td>
            <td>@obj.Price</td>
        </tr>
        }
        
    </tbody>
    </table>
</div>